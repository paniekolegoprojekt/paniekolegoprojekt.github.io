{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "@": ["./src"],
      "@/*": ["./src/*"],
      "atoms": ["./src/ui/atoms"],
      "$ui/*": ["./src/ui/*"]
    }
  },
  "include": ["vite.config.ts"]
}
